<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<BorderPane fx:controller="kr.or.ddit.basic.T11_ComboGugudanController" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>	<!-- ctrl + 1을 눌러주자 -->
            <ComboBox fx:id="cmdDan" prefWidth="150.0" />
            <Button onAction="#btnDanClick" fx:id="btnDan" mnemonicParsing="false" text="출력" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <TextArea fx:id="txtResult" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
